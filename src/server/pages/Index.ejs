<div class="content">
    <div class="randombots">
        <h2 style="color: #ffffff;"><strong>Боты по рейтингу</strong></h2>
        <% for(let i = 0; i < bots.length; i++) { %>
            <div class="row">
                <% for(let j = 0; j < bots[i].length; j++) { %>
                    <style>
                        .bot-card-col{
                         transition: all 0.5s ease-out;
                         }
                         .bot-card-col:hover{
                          transform: scale(1.1);
                         }
                    </style>
                    <div  class="col-md-3 mb-3 bot-card-col">
                        <div style="background: #23272A;border-radius: 0 20px;" class="card">
                            <% if (bots[i][j].avatar) { %>
                                <img style="border-radius: 0 20px;" class="card-img-top" src="https://cdn.discordapp.com/avatars/<%= bots[i][j].botID %>/<%= bots[i][j].avatar %>.png?size=1280" alt="<%= bots[i][j].tag %>">
                            <% } else { %>
                                <img style="border-radius: 0 20px;" class="card-img-top" src="https://cdn.discordapp.com/embed/avatars/<%= (bots[i][j].botID % 5) %>.png" alt="<%= bots[i][j].tag %>">
                            <% } %>
                            <div style="border-radius: 10px;" class="card-body">
                                <h4 class="card-title" style="margin-bottom: 0;border-radius: 0 10px;color: #ffffff;"><strong><%= bots[i][j].tag %></strong></h4>

                                    <div class="badges">
                                        <span style="border-radius: 0 10px;padding-right: 15px;padding-left: 15px;" class="badge badge-success">Онлайн</span>
                                        <span style="border-radius: 0 10px;padding-right: 15px;padding-left: 15px;" class="badge badge-primary">Серверов: ?</span>
                                        <span style="border-radius: 0 10px;padding-right: 15px;padding-left: 15px;" class="badge badge-danger">Голосов: <%= bots[i][j].upvotes %></span>
                                    </div>
                                
                             

                                <p class="card-text" style="color: rgb(255, 255, 255);"><strong><%= bots[i][j].shortDesc.length > 5 ? bots[i][j].shortDesc.substr(0, 97) + "..." : bots[i][j].shortDesc %></strong></p>
                                <div class="button-view" id="bot-card-bottons" role="group" aria-label="Basic example" style="width: 100%;">
                                     <a href="/bots/<%= bots[i][j].botID %>" style="border-radius: 15px;background-color: #ffffff;color: black;" type="button" class="btn btn-primary">Просмотр</a>  
                                </div>
                            </div>
                        </div>
                    </div>
                <% } %>
            </div>
        <% } %>
    </div>

